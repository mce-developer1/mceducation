<div class="app-content page-{{module}} {{#unless assignmentsPage}}no-margin{{/unless}}">
  <div class="row no-gutters">
    <div class="col-sm">
      <div class="article article-track-test-user no-sidebar">
        {{#if assignmentsPage}}
          <div class="article-header">
            <nav class="navbar navbar-expand">
              <ul class="navbar-nav">
                <li class="nav-item nav-item-drawer">
                  <div class="item-text">
                    <div class="text-truncate resource-title">Making Number Patterns</div>
                    <div class="text-muted text-truncate">Grade 3, Mathematics</div>
                  </div>
                </li>
              </ul>
              <ul class="navbar-nav nav-main"></ul>
              <ul class="navbar-nav"></ul>
            </nav>
          </div>
        {{/if}}
        <div class="article-content">          
          <div class="article-body">
            <div class="user-test-result">
              <div class="user-details">
                <img src="/static/images/user.png">
                <div class="user-summary">
                  <div class="user-name text-truncate">Aaron Tan</div>
                  <div class="text-muted">P1-EA</div>
                </div>
              </div>
              <div class="chart">
                <div class="chart-area">
                </div>
                <div class="chart-legend">
                  <div class="legend-group">
                    <div class="legend-label">Submission Date:</div>
                    <div class="legend-value legend-value-md">18 Jun 2019</div>
                  </div>
                  <div class="legend-group">
                    <div class="legend-label">Duration:</div>
                    <div class="legend-value legend-value-md">20 mins</div>
                  </div>
                  <div class="legend-group">
                    <div class="legend-label">Score:</div>
                    <div class="legend-value">70%</div>
                    <button class="btn btn-light btn-view-results" type="button">
                      <i class="fas fa-clipboard-check"></i>
                    </button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
